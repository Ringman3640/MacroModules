<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <Brush x:Key="Module.Icon.Color">White</Brush>
    <Brush x:Key="Module.InputType.BodyColor">#CA2121</Brush>
    <Brush x:Key="Module.MouseType.BodyColor">#CA6D21</Brush>
    <Brush x:Key="Module.ProgramType.BodyColor">#224187</Brush>
    <Brush x:Key="Module.ExecutionType.BodyColor">#CACA21</Brush>
    <Brush x:Key="Module.OtherType.BodyColor">#611C87</Brush>
    <Brush x:Key="Module.EntryType.BodyColor">#1AA21A</Brush>

    <SolidColorBrush x:Key="FormElement.Background">#97BDC9</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.Border">#97BDC9</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.MouseOver.Background">#87A6B0</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.MouseOver.Border">#87A6B0</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.Pressed.Background">#87A6B0</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.Pressed.Border">#87A6B0</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.Disabled.Background">#92A6AD</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.Disabled.Border">#92A6AD</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.Focus.Border">#87A6B0</SolidColorBrush>
</ResourceDictionary>