<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <Thickness x:Key="Padding.Normal">10</Thickness>
    <Thickness x:Key="Padding.Small">4</Thickness>
    <Thickness x:Key="Margin.Normal">10</Thickness>
    <Thickness x:Key="Margin.Small">4</Thickness>
    <sys:Double x:Key="Module.Width">80</sys:Double>
    <sys:Double x:Key="Module.Height">80</sys:Double>
    <sys:Double x:Key="Module.Icon.Width">50</sys:Double>
    <sys:Double x:Key="Module.Icon.Height">50</sys:Double>
    <CornerRadius x:Key="CornerRadius.Small">4</CornerRadius>
    <CornerRadius x:Key="CornerRadius.Medium">10</CornerRadius>
    <CornerRadius x:Key="CornerRadius.Large">16</CornerRadius>
    <sys:Double x:Key="FontSize.Medium">16</sys:Double>
    <sys:Double x:Key="FontSize.Normal">14</sys:Double>
    
    <sys:Double x:Key="FormElement.FontSize">14</sys:Double>
    <Thickness x:Key="FormElement.Padding">2</Thickness>
    <Thickness x:Key="FormButton.Padding">12, 4, 12, 4</Thickness>
</ResourceDictionary>