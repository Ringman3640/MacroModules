                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <Thickness x:Key="StandardPadding">10</Thickness>
    <sys:Double x:Key="Module.Width">80</sys:Double>
    <sys:Double x:Key="Module.Height">80</sys:Double>
    <sys:Double x:Key="Module.Icon.Width">50</sys:Double>
    <sys:Double x:Key="Module.Icon.Height">50</sys:Double>
    <CornerRadius x:Key="CornerRadius.Small">4</CornerRadius>
    <CornerRadius x:Key="CornerRadius.Medium">10</CornerRadius>
    <CornerRadius x:Key="CornerRadius.Large">16</CornerRadius>
    <sys:Double x:Key="FontSize.Medium">16</sys:Double>
</ResourceDictionary>