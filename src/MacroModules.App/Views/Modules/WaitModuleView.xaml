<UserControl x:Class="MacroModules.App.Views.Modules.WaitModuleView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MacroModules.App.Views.Modules"
             xmlns:moduleVMs="clr-namespace:MacroModules.App.ViewModels.Modules"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=moduleVMs:WaitModuleVM}"
             d:DesignHeight="450" d:DesignWidth="800">
    <local:BaseModuleView BodyColor="{StaticResource StandardExecutionModuleColor}">
        <local:BaseModuleView.BodyContent>
            <Image Source="{StaticResource icon_waitDrawingImage}"/>
        </local:BaseModuleView.BodyContent>
    </local:BaseModuleView>
</UserControl>
