<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <Brush x:Key="Module.Icon.Color">White</Brush>
    <Brush x:Key="Module.InputType.BodyColor">#CA2121</Brush>
    <Brush x:Key="Module.MouseType.BodyColor">#CA6D21</Brush>
    <Brush x:Key="Module.ProgramType.BodyColor">#224187</Brush>
    <Brush x:Key="Module.ExecutionType.BodyColor">#CACA21</Brush>
    <Brush x:Key="Module.OtherType.BodyColor">#611C87</Brush>
    <Brush x:Key="Module.EntryType.BodyColor">#1AA21A</Brush>

    <SolidColorBrush x:Key="FormElement.Background">#20000000</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.BorderColor">#20000000</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.MouseOver.BorderColor">#C6D2E6</SolidColorBrush>
    <SolidColorBrush x:Key="FormElement.Focus.BorderColor">#C6D2E6</SolidColorBrush>
</ResourceDictionary>